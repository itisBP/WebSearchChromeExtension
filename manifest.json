{  
    "manifest_version": 2,  
    "name": "Web Searcher",  
    "description": "Extension for searching different websites on right click",  
    "version": "1.2",  
    "browser_action": { 
        "default_popup": "popup.html"  
    },  
    
    "background": { 
        "scripts": ["script.js"]
    },
    "permissions": ["activeTab", "contextMenus"],
    "icons": {"16": "Images/WebSearcher.jpg", "48": "Images/WebSearcher.jpg"}
}